{
  "name": "react-tailwind",
  "version": "3.0.0",
  "description": "Create React App with Tailwind CSS",
  "license": "MIT",
  "author": {
    "name": "Amit Agarwal",
    "email": "amit@labnol.org",
    "url": "https://www.labnol.org"
  },
  "private": false,
  "scripts": {
    "dev:tailwind": "tailwindcss -i src/styles/tailwind.css -o src/styles/index.css -w",
    "start": "react-scripts start",
    "tailwind:build": "NODE_ENV=production tailwindcss -i src/styles/tailwind.css -o src/styles/index.css --minify",
    "prebuild": " npm run tailwind:build",
    "build": "react-scripts build"
  },
  "keywords": [
    "react",
    "tailwindcss",
    "tailwindui",
    "create-react-app"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.1",
    "antd": "^4.18.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "tailwindcss": "^3.0.19",
    "react-scripts": "^5.0.0"
  }
}
